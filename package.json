{
  "name": "RKStudio Cypress UI Tests",
  "version": "1.0.0",
  "description": "This framework for testing UI",
  "main": "index.js",
  "scripts": {
    "cy:run": "TZ=America/Denver ./node_modules/.bin/cypress run --browser chrome --env allure=true",
    "cy:open": "TZ=America/Denver ./node_modules/.bin/cypress open",
    "cy:instal": "TZ=America/Denver ./node_modules/.bin/cypress install",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "allure:clear": "rm -r allure-results/ allure-report rkstudio/logs/ || true",
    "allure:server": "allure serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress-real-events": "^1.7.0",
    "@bahmutov/all-logs": "1.8.1",
    "@bahmutov/cy-api": "^2.1.3",
    "chai-jquery": "^2.1.0",
    "cypress": "10.6.0",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-wait-until": "^1.6.0",
    "dayjs": "^1.10.6",
    "dotenv": "10.0.0 ",
    "form-data": "^2.3.3",
    "fs": "^0.0.1-security",
    "json-update": "^5.3.0",
    "mocha-junit-reporter": "^2.0.0",
    "sleep": "^6.3.0",
    "tr_cypress_reporter_simple": "1.0.18",
    "unirest": "^0.6.0",
    "xmlhttprequest": "^1.8.0",
    "esm": "^3.2.25",
    "@shelex/cypress-allure-plugin": "^2.28.0",
    "allure-commandline": "^2.18.1",
    "cypress-file-upload": "^5.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/npm.git"
  },
